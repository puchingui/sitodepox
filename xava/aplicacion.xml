<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd"><aplicacion nombre="Sitodep"> 		<!--	Un m�dulo por defecto se asume para cada	controlador en <modulo-defecto/> 	-->	<modulo-defecto>		  		<controlador nombre="Sitodep"/>			</modulo-defecto>		<!--	Podemos definir m�dulos adicionales, por ejemplo: 		<modulo nombre="ProfesorSoloLectura">		<modelo nombre="Profesor"/>		<controlador nombre="Print"/>	</modulo>			-->	  		<modulo nombre="ConsultarOrdenes">		<var-entorno nombre="XAVA_SEARCH_ACTION" valor="CRUD.searchReadOnly"/>		<modelo nombre="OrdenTrabajo"/>		<controlador nombre="Print"/>	</modulo>		<modulo nombre="Archivo">		<var-entorno nombre="XAVA_LIST_ACTION" valor="Archivo.restaurar"/>		<modelo nombre="Producto"/>		<tab nombre="Archivo"/>		<controlador nombre="Archivo"/>		<controlador-modo nombre="ListOnly"/>	</modulo>		</aplicacion>